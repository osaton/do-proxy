{
  "name": "do-storage",
  "version": "0.0.0",
  "type": "module",
  "module": "./dist/do-storage.js",
  "main": "./dist/do-storage.cjs",
  "types": "./dist/do-storage.d.ts",
  "files": [
    "dist"
  ],
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/osaton/do-storage"
  },
  "tsup": {
    "entry": [
      "src/do-storage.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": true,
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  },
  "author": "osaton <github@osasto.dev>",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@types/jest": "^29.1.2",
    "esbuild": "^0.15.15",
    "jest": "^29.2.0",
    "jest-environment-miniflare": "^2.10.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3",
    "wrangler": "2.4.4"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1"
  },
  "scripts": {
    "doc": "typedoc src/do-storage.ts",
    "prepublish": "npm run test && npm run build",
    "preversion": "npm run test && npm run build",
    "start": "wrangler dev",
    "build": "rimraf dist && tsup",
    "build-test": "node build-test.js",
    "deploy": "wrangler publish",
    "test": "npm run build-test && node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js"
  }
}
